  print("False")


12.
is_raining = True
is_windy = False

if is_raining == True and is_windy == False:
  print("DONT GO OUTSIDE")


13.
is_raining = True
is_windy = False

if is_raining == False and is_windy == False:
  print("You can go outside.")


14.
is_raining = True
is_windy = False

if not is_raining == False:
  print("It's raininhg")


