palin = input("Enter a string: ")
nilap = palin[::-1]
if palin == nilap:
    print("Your string is a palindrome.")
else:
    print("Your string is not a palindrome.")


7.
sentence = input("Enter a sentance: ")
sentence = sentence.lower()
words = sentence.split()
word_dict = {}
for word in words:
    word = word.strip(",.!?")
    if word in word_dict:
        word_dict[word] += 1
    else:
        word_dict[word] = 1
print(word_dict)


8.
strung = input("Enter a string: ")
char_count = {}
for char in strung:
    if char != ' ':
        if char in char_count:
            char_count[char] += 1
        else:
            char_count[char] = 1
most_char = max(char_count, key=char_count.get)
print(most_char)


9.
dict1 = {"a":1, "b": 2, "c": 3}
dict2 = {"d": 4, "e": 5, "f": 6}
dict3 = dict1 | dict2
print(dict3)


10.
tup1 = ("Alice", 12, "Diamond Bar")
tup2 = ("Isabel", 11, "Chino Hills")
tup3 = tup1 + tup2
a,b,c,d,e,f = tup3
tup1 = (a,b,c)
tup2 = (d,e,f)
print(tup1)
print(tup2)
print(tup3)
