ages = {'Alice': 30, 'Bob': 25, 'Charlie': 30, 'Diana': 22}
del ages['Alice']
del ages['Bob']
del ages['Charlie']
del ages['Diana']
ages[30] = "Alice"
ages[25] = "Bob"
ages[30] = "Charlie"
ages[22] = "Diana"
print(ages)


